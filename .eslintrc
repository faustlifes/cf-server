{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "es6":   true
    },
    "rules": {
        "no-plusplus":"off",
        "no-console":"off",
        "no-undef": "off",
        "class-methods-use-this":"off",
        "no-prototype-builtins":"off",
        "max-len": [2, {"code": 150, "tabWidth": 4, "ignoreUrls": true}],
        "space-before-function-paren": ["error", {
            "anonymous": "always",
            "named": "always",
            "asyncArrow": "ignore"
        }]
    }
}
